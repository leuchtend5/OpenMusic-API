{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "max-len": ["error", { "code": 100 }],
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "object-curly-newline": "off"
  }
}
